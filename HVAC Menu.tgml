/**==========================================================================================
 * Event Callback Functions
 */

function load(evt){	
	main.load(evt);
}

function down(evt) {
	main.down(evt);
}

function up(evt) {
	main.up(evt);
}

function out(evt) {
	main.out(evt);
}

function move(evt) {
	main.move(evt);
}

function click(evt) {
	main.click(evt);
}

/**==========================================================================================
 * Controller Object
 */
Controller = {
	init: function(comp) {
		this.tgml = comp;
		this.library = this.tgml.getChild("Library");
		this.document = this.tgml.getOwnerDocument();
		
		this.startY = 0;
		this.startX = 0;
		this.startTime = 0;
		this.activeView = 0;
		this.left = 0;
		this.gesture = false;
		
		this.width = +comp.getAttribute("Width");
		this.height = +comp.getAttribute("Height");
		
		var boxList = comp.getChild("VAVs").getChildNodes();
		this.boxes = {};
		this.keys = [];
		for(var i = 0, l = boxList.length; i < l; i++) {
			var child = boxList.item(i);
			var name = String(child.getAttribute("Name"));
			if(String(child.getTagName()) === "Component") {
				this.keys.push(name);
				this.boxes[name] = Object.construct(Box, child);
			}
		}
		
//		this.hoaLink = this.document.getElementById("MobileHOALink").getElementsByTagName("Link").item(0).getFullBindName();
//		this.testLink = this.document.getElementById("MobileTestLink").getElementsByTagName("Link").item(0).getFullBindName();
		
//		this.navBar = Object.construct(NavBar, comp.getChild("NavBar"));
		this.bgd = Object.construct(Background, comp.getChild("Background"));
//		this.footer = Object.construct(Footer, comp.getChild("Footer"));
		
//		this.headerHeight = this.navBar.height + this.bgd.height;
//		this.footerTop = this.footer.top;
		
		this.activeScreen = 0;
		this.createBoxScreen();
	//	this.createTestScreen(); 
	},
	
	update: function(signal) {	
	},
	
	createBoxScreen: function() {
		// make viewable area
		this.boxScreenView = this.document.createElement("Component");
		
		this.boxScreenView.setAttribute("Height", this.footerTop - this.headerHeight);
		this.boxScreenView.setAttribute("Width", this.width);
		this.boxScreenView.setAttribute("Left", 0);
		this.boxScreenView.setAttribute("Top", this.headerHeight);		
		this.boxScreenView.setAttribute("ContentHeight", this.footerTop - this.headerHeight);
		this.boxScreenView.setAttribute("ContentWidth", this.width);
		// clip to true to hide outside viewable area
		this.boxScreenView.setAttribute("Clip", "True");
		
		this.tgml.appendChild(this.boxScreenView);
		
		// create BoxList component
		this.boxScreen = this.document.createElement("Component");
		
		this.boxTop = 0;
		this.boxHeight = (this.keys.length) * (+this.library.getChild("Item").getAttribute("Height"));
			
		this.boxScreen.setAttribute("Height", this.footerTop - this.headerHeight);
		this.boxScreen.setAttribute("Width", this.width);
		this.boxScreen.setAttribute("Left", 0);
		this.boxScreen.setAttribute("Top", this.boxTop);		
		this.boxScreen.setAttribute("ContentHeight", this.footerTop - this.headerHeight);
		this.boxScreen.setAttribute("ContentWidth", this.width);
		this.boxScreen.setAttribute("Clip", "False");
		
		this.boxScreenView.appendChild(this.boxScreen);
		
		this.boxItems = [];
		
		for(var i = 0, l = this.keys.length; i < l; i++) {
			var boxItem = this.library.getChild("Item").cloneNode(true);
			this.boxScreen.appendChild(boxItem);
			this.boxItems.push(Object.construct(Item, boxItem, this.keys[i], i));
		}
	},
	
	createTestScreen: function() {
		this.testScreen = this.document.createElement("Component");
			
		this.testScreen.setAttribute("Height", this.footerTop - this.headerHeight);
		this.testScreen.setAttribute("Width", this.width);
		this.testScreen.setAttribute("Left", 0);
		this.testScreen.setAttribute("Top", this.headerHeight);		
		this.testScreen.setAttribute("ContentHeight", this.footerTop - this.headerHeight);
		this.testScreen.setAttribute("ContentWidth", this.width);
		this.testScreen.setAttribute("Visibility", "Hidden");
		
		this.tgml.appendChild(this.testScreen);
		
		// make a Test button
		var testButton = this.library.getChild("Button").cloneNode(true);
		this.testScreen.appendChild(testButton);
		this.testBtn = Object.construct(Button, testButton, "Test");
		this.testBtn.node.setAttribute("Left", (this.width - this.testBtn.width)/2);
		this.testBtn.node.setAttribute("Top", (this.footerTop) * 1/3 - this.headerHeight);		
		// make a HOA button
		var hoaButton = this.library.getChild("Button").cloneNode(true);
		this.testScreen.appendChild(hoaButton);
		this.hoaBtn = Object.construct(Button, hoaButton, "HOA");
		this.hoaBtn.node.setAttribute("Left", (this.width - this.hoaBtn.width)/2);
		this.hoaBtn.node.setAttribute("Top", (this.footerTop) * 2/3 - this.headerHeight);
	},
	
	mouseDown: function(mouse) {
			this.startY = mouse.y;
			this.startX = mouse.x;
			this.startTime = new Date();			
	},
	
	mouseMove: function(mouse) {
		if(mouse.down) {
			var xOffset = mouse.x - this.startX;
			var yOffset = mouse.y - this.startY;
			
			// give Wiggle Room
			if((Math.abs(xOffset) > 5 || Math.abs(yOffset) > 5) && mouse.y > this.headerHeight && mouse.y < this.footerTop) {
				this.gesture = true;
			}
		
			if(Math.abs(yOffset) > Math.abs(xOffset)) {
				if(this.activeScreen === 0) {
					this.boxScreen.setAttribute("Top", this.boxTop + yOffset);
				}
			}
		}
	},
	
	mouseUp: function(mouse) {	
		if(!this.gesture && mouse.y < this.footerTop) {		
		
			var target = String(mouse.target.getAttribute("Name"));
			if(this.boxes.hasOwnProperty(target)) {		
				this.boxes[target].gotoLink();
			}
		
			switch(target) {
			case "BoxesNav":
				this.activeScreen = 0;
				break;
			case "TestNav":
				this.activeScreen = 1;	
				break;
			case "Test":
				invoke(this.testLink,"ViewObject");
				break;
			case "HOA":
				invoke(this.hoaLink,"ViewObject");
				break;
			default:
				break;
			}
			
			this.updateScreens();
		}
	},
	
	gestureFinished: function(mouse) {
		if(this.gesture) {
			var finishX = mouse.x;
			var finishY = mouse.y;
			var finishTime = new Date();
			var deltaX = finishX - this.startX;
			var deltaY = finishY - this.startY;
			
			var distance = Math.sqrt(Math.pow(deltaX,2) + Math.pow(deltaY,2));
			var time = (finishTime - this.startTime)/1000;
			
			var velocity = distance/time;
			var move = 0;
			if(velocity > 1500) {
				if(Math.abs(deltaX) > Math.abs(deltaY)) {
					if(deltaX > 0) {
						move = -1;
					} else {
						move = 1;
					}
				}
			} else {
				if(Math.abs(deltaX) > this.width/2) {
					if(deltaX > 0) {
						move = -1;
					} else {
						move = 1;
					}
				}
			}
			
			this.activeScreen = Math.min(Math.max(this.activeScreen + move,0),0);
			this.updateScreens();
		
			this.gesture = false;
		}
		this.boxTop = +this.boxScreen.getAttribute("Top");
		if(this.boxHeight > (this.footerTop - this.headerHeight)) {
			this.boxScreen.setAttribute("Top", Math.max(Math.min(this.boxTop, 0), (this.footerTop - this.headerHeight - this.boxHeight)));
		} else {
			this.boxScreen.setAttribute("Top", 0);
		}
		this.boxTop = +this.boxScreen.getAttribute("Top");
	},
	
	updateScreens: function() {
		this.boxScreen.setAttribute("Visibility", this.activeScreen === 0 ? "Visible" : "Hidden");
	//	this.testScreen.setAttribute("Visibility", this.activeScreen === 1 ? "Visible" : "Hidden");
		this.navBar.update(this.activeScreen);
	}
};

/**==========================================================================================
 * Box Object
 */
Box = {
	init: function(node) {		
		this.link = node.getElementsByTagName("Link").item(0).getFullBindName();
	},
	
	gotoLink: function(node) {
		invoke(this.link, "ViewObject");
	}
};

/**==========================================================================================
 * Item Object
 */
Item = {
	init: function(node, name, index) {
		this.node = node;
		this.height = +this.node.getAttribute("Height");
		this.textBox = this.node.getElementsByTagName("TextBox").item(0);	
		this.textBox.setAttribute("Content", name);
		this.node.setAttribute("Top", index*this.height);
		this.node.setAttribute("Left", "0.0");
		this.node.getElementsByTagName("TargetArea").item(0).setAttribute("Name", name);
		this.active = false;
		
		this.bgd = this.node.getChild("bgd");
	}
};

/**==========================================================================================
 * Button Object
 */
Button = {
	init: function(node, name) {
		this.node = node;
		this.node.getElementsByTagName("TargetArea").item(0).setAttribute("Name", name);
		this.width = +this.node.getAttribute("Width");
		this.height = +this.node.getAttribute("Height");
		this.textBox = this.node.getElementsByTagName("TextBox").item(0);	
		this.textBox.setAttribute("Content", name);
	}
};

/**==========================================================================================
 * NavBar Object
 */
NavBar = {
	init: function(node) {		
		var navList = node.getChildNodes();
		this.navs = [];
		for(var i = 0, l = navList.length; i < l; i++) {
			var child = navList.item(i);
			if(String(child.getTagName()) === "Component") {
				this.navs.push(Object.construct(Nav, child));
			}
		}
		
		this.height = this.navs[0].height;
	},
	update: function(val) {
		var i = this.navs.length;
		while(i--) {
			this.navs[i].update(i === val);
		}
	}
};

/**==========================================================================================
 * Nav Object
 */
Nav = {
	init: function(node) {
		this.node = node;
		this.name = String(this.node.getAttribute("Name"));
		this.height = +this.node.getAttribute("Height");
		this.underline = this.node.getElementsByTagName("Rectangle").item(0);
	},
	update: function(val) {
		if(this.underline) {
			this.underline.setAttribute("Visibility", val ? "Visible" : "Hidden");
		}
	}
};

